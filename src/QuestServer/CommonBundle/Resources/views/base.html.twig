<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>{% block title %}Quest App{% endblock %}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">

    {% block stylesheets %}
	{% if nojs is not defined %}
    <!-- Le styles -->
    <link href="{{ asset('bundles/questservercommon/assets/css/flatly.bootstrap.min.css') }}" rel="stylesheet">
    <style type="text/css">
      body {
       /* padding-top: 60px;*/
        padding-bottom: 40px;
		font-size: 12px;
      }
	  label {
		font-size: 12px;
	  }
	  .collection-item {
		float: none;
		clear: both;
	  }
	  .collection-item .control-group{
		float: left;
	  }
	  .collection-item input{
		width: 50px;
	  }
	  .filterform > div {
		  float: left;
		  margin: 5px;
	  }
	  .filterform > .btn {
		  float: none;
	  }
    </style>
    <link href="{{ asset('bundles/questservercommon/assets/css/bootstrap-responsive.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/questservercommon/assets/css/jquery-ui.min.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/questservercommon/assets/css/select2.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/questservercommon/assets/css/DT_bootstrap.css') }}" rel="stylesheet">
	{% endif %}
    {% endblock %}

    <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="../assets/js/html5shiv.js"></script>
    <![endif]-->
    {% block javascripts %}
		<script src="{{ asset('bundles/questservercommon/assets/js/jquery.min.js') }}"></script>
		<script src="{{ asset('bundles/questservercommon/assets/js/jquery-ui.min.js') }}"></script>
		<script src="{{ asset('bundles/questservercommon/assets/js/bootstrap.min.js') }}"></script>
		<script src="{{ asset('bundles/questservercommon/assets/js/select2.min.js') }}"></script>
		<script src="{{ asset('bundles/questservercommon/assets/js/jquery.dataTables.js') }}"></script>
		<script src="{{ asset('bundles/questservercommon/assets/js/DT_bootstrap.js') }}"></script>
    {% endblock %}

    <!-- Fav and touch icons -->
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="{{ asset('bundles/questservercommon/assets/ico/apple-touch-icon-144-precomposed.png') }}">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="{{ asset('bundles/questservercommon/assets/ico/apple-touch-icon-114-precomposed.png') }}">
      <link rel="apple-touch-icon-precomposed" sizes="72x72" href="{{ asset('bundles/questservercommon/assets/ico/apple-touch-icon-72-precomposed.png') }}">
                    <link rel="apple-touch-icon-precomposed" href="{{ asset('bundles/questservercommon/assets/ico/apple-touch-icon-57-precomposed.png') }}">
                                   <link rel="shortcut icon" href="{{ asset('bundles/questservercommon/assets/ico/favicon.png') }}">
								   
	   <link rel="stylesheet" href="{{ asset('bundles/questservercommon/font-awesome/css/font-awesome.min.css') }}">

  </head>

  <body>

	{% block body_in %}
	    <div class="container">
		{% block container_in %}
		{% endblock %}
		</div>
	{% endblock %}
	
	<div id="modal" class="modal hide fade" tabindex="-1" role="dialog" aria-hidden="true">
	  <div class="modal-body">
	    Loading . . .
	  </div>
	  <div class="modal-footer">
	    <button class="btn" data-dismiss="modal" aria-hidden="true">{% trans %}Close{% endtrans %}</button>
	  </div>
	</div>
  </body>
</html>
